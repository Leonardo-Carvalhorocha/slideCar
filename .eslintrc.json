{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        
    },
    "rules": {
        "no-shadow": 0,
        "import/extensions": 0,
        "array-callback-return": 0,
        "no-param-reassign": 0,
        "max-classes-per-file": 0,
        "new-cap": 0,
        "no-console": 0
    }
}
